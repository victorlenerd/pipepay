{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for pipepay product.",
  "scripts": {
    "debug": "NODE_ENV=development node --inspect ./dist/server.js",
    "dev": "NODE_ENV=development node --inspect node_modules/webpack/bin/webpack.js --progress --colors --config config.dev.js",
    "start": "NODE_ENV=production forever ./build/server.js",
    "build": "NODE_ENV=production webpack --progress --colors config.prod.js",
    "test": "NODE_ENV=testing rm -rf ./tmp/mocha-webpack && nyc --reporter=text mocha-webpack --webpack-config config.test.js  \"src/**/*.spec.js\" --timeout 10000 --require source-map-support/register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amazon-cognito-identity-js": "^2.0.20",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "date-fns": "^1.29.0",
    "email-templates": "^5.0.0",
    "express": "^4.16.3",
    "fetch": "^1.1.0",
    "jsonwebtoken": "^8.2.1",
    "lodash.find": "^4.6.0",
    "lodash.merge": "^4.6.1",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.7",
    "pug": "^2.0.3",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "dotenv-webpack": "^1.5.7",
    "eslint-plugin-babel": "^5.1.0",
    "graphql-tag": "^2.8.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^4.0.1",
    "mocha-webpack": "next",
    "node-fetch-polyfill": "^2.0.6",
    "nyc": "^13.0.1",
    "raw-loader": "^0.5.1",
    "source-map-support": "^0.5.0",
    "start-server-webpack-plugin": "^2.2.5",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "sourceType": "module",
    "instrument": false,
    "sourceMap": false
  }
}
